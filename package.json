        "cookie-parser": "~1.0.1",
        "body-parser": "~1.0.0",
        "debug": "~0.7.4",
        "monk": "~0.8.1",
        "jade": "~1.3.0",
        "request": "~2.36.0",
        "qs": "~0.6.6",
        "node-schedule": "~0.1.13",
        "passport": "~0.2.0",
        "passport-local": "~1.0.0",
        "feedparser": "~0.17.0",
        "winston": "~0.7.3",
        "connect-flash": "~0.1.1",
        "underscore": "~1.6.0",
        "browserify-middleware": "~2.8.0",
        "jadeify": "~2.5.0",
        "jquery": "~1.11.1",
        "jquery-ui": "~1.10.5",
        "metismenu": "~1.1.1",
        "bootstrap": "~3.1.1",

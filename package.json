{
    "name": "teamate",
    "description": "team mate monitor team task' status",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node app.js",
        "dev": "npm run watch & nodemon app.js",
        "build": "browserify -t brfs -t uglifyify modules/browser.js -o public/js/teamate/bundle.js",

extends layout
block content
	.row
		.col-lg-12
			h1.page-header My Events
			#event-editor
				.modal-dialog.modal-sm
					.modal-content
						.modal-header
							button.close(onclick='closeEventEditor();') &times;
							h4.modal-title Title
						.modal-body
							p body
						.modal-footer
							button#event-save-btn.btn.btn-primary(type='button') Save
			#calendar
